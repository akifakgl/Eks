<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bes"
    tools:context="com.example.gulof.bitirme_1.bolum_3">


    <ImageView
        android:id="@+id/uc1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="133dp"
        app:layout_constraintEnd_toStartOf="@+id/uc3"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ucunbiri" />

    <ImageView
        android:id="@+id/uc2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        app:layout_constraintEnd_toStartOf="@+id/uc4"
        app:layout_constraintTop_toBottomOf="@+id/uc1"
        app:srcCompat="@drawable/ucunikisi" />

    <ImageView
        android:id="@+id/uc3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/uc1"
        app:layout_constraintTop_toTopOf="@+id/uc1"
        app:srcCompat="@drawable/ucunucu" />

    <ImageView
        android:id="@+id/uc4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/uc2"
        app:layout_constraintStart_toEndOf="@+id/uc1"
        app:layout_constraintTop_toTopOf="@+id/uc2"
        app:srcCompat="@drawable/ucundordu" />

    <ImageView
        android:id="@+id/uc5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/uc2"
        app:layout_constraintStart_toEndOf="@+id/uc2"
        app:srcCompat="@drawable/ucunbesi" />
</android.support.constraint.ConstraintLayout>
